<div class="wrapper">
	<%= render "shared/regheader" %>
	<div class="content-wrapper">
			<div class="banner">
				<% if flash[:alert] %>
					<p><%= flash[:alert] %></p>
				<% end %>
			<div id="welcome">Welcome Back!</div>
			<%= simple_form_for :session, :url => sessions_path do |f| %>
				<%= f.input :email %>
				<%= f.input :password %>
				<%= f.submit 'Log In', class: "button_save", id: "loginbutton" %>
			<% end %>
			<h5><%= link_to "Sign Up", new_user_path %></h5>
			</div>
	</div>
<%= render "shared/regfooter" %>
</div>

